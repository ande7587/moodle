<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/library_reserves/db" VERSION="20140627" COMMENT="XMLDB file for Moodle local/library_reserves"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_library_reserve" COMMENT="Intersection entity between local_library_resource and ppsft_classes">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="ppsft_class_id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="resource_id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="sort_number" TYPE="int" LENGTH="3" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="ppsft_class_id-resource_id" TYPE="unique" FIELDS="ppsft_class_id, resource_id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="sort_number" UNIQUE="false" FIELDS="sort_number"/>
        <INDEX NAME="timemodified" UNIQUE="false" FIELDS="timemodified"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="local_library_resource" COMMENT="Independent entity represent a resource from the library">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="resource_id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="title" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="resource_type" TYPE="char" LENGTH="10" NOTNULL="false" DEFAULT="item" SEQUENCE="false"/>
        <FIELD NAME="url" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="note" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="resource_id" TYPE="unique" FIELDS="resource_id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="resource_type" UNIQUE="false" FIELDS="resource_type"/>
        <INDEX NAME="timemodified" UNIQUE="false" FIELDS="timemodified"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
